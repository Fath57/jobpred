model AppFile {
  id         String   @id @default(cuid())

  // Champs demand√©s
  name       String        
  extension  String        
  size       BigInt        
  path       String  @unique  
  url        String?        

  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt

  candidates Candidate[]
  @@map("app_files")
}
