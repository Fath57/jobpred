{
  "name": "jobprep-backend",
  "version": "1.0.0",
  "description": "JobPrep Backend API - Plateforme IA de prÃ©paration aux entretiens",
  "main": "dist/index.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:docker": "yarn db:generate && docker-compose -f docker-compose-local.yml up -d",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:watch": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:init": "ts-node -r tsconfig-paths/register scripts/init-db.ts",
    "generate:secrets": "node scripts/generate-secrets.js",
    "db:migrate:reset": "npx prisma migrate reset --schema=prisma/schema.prisma",
    "db:migrate:deploy": "npx prisma migrate deploy --schema=prisma/schema.prisma",
    "db:dockerreset": "docker compose -f docker-compose-local.yml down -v && docker compose -f docker-compose-local.yml up -d && sleep 3 && npm run db:migrate:deploy",
    "stop:docker": "docker compose -f docker-compose-local.yml down",
    "db:build:schema": "node build-schema.js",
    "db:generate": "npm run db:build:schema && npx prisma generate --schema=prisma/schema.prisma && prisma migrate dev --schema=prisma/schema.prisma",
    "db:generate:schema": "npx prisma generate --schema=prisma/schema.prisma",
    "db:migrate:dev": "prisma migrate dev --schema=prisma/schema.prisma",
    "db:studio": "prisma studio --schema=prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema=prisma/schema.prisma",
    "db:seed": "npm run db:generate:schema && ts-node prisma/seeders/index.ts",
    "docker:build": "docker-compose -f docker-compose.yml -f docker-compose.development.yml build",
    "docker:up": "docker-compose -f docker-compose.yml -f docker-compose.development.yml up",
    "docker:down": "docker-compose -f docker-compose.yml -f docker-compose.development.yml down"
  },
  "keywords": [
    "jobprep",
    "ai",
    "interview",
    "resume",
    "backend",
    "api"
  ],
  "author": "JobPrep Team",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@google/generative-ai": "^0.24.1",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^5.2.0",
    "@prisma/client": "^6.16.2",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "faker-js": "^1.0.0",
    "helmet": "^7.2.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "openai": "^6.8.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "stripe": "^14.14.0",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.17.20",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.19",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "prisma": "^6.16.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
